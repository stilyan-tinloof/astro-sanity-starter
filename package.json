{
	"name": "sanity-starter",
	"description": "A Next.js and Sanity CMS starter with Turbo monorepo structure",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"format-and-lint": "biome check .",
		"format-and-lint:fix": "biome check . --write",
		"check-types": "turbo run check-types",
		"typegen": "turbo run typegen"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"turbo": "^2.6.3",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "pnpm@10.25.0"
}
